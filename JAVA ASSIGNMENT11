import java.util.*;
class Minmax
{
public static void main(String args[])
{
int n,i,min,max;
Scanner sc=new Scanner(System.in);
System.out.print("enter array size");
n=sc.nextInt();
int ar[]=new int[n];
System.out.print("enter array elements");
for(i=0;i<n;i++)
{
ar[i]=sc.nextInt();
}
min=ar[0];
max=ar[0];
for(i=1;i<n;i++)
{
if(ar[i]<min)
{
min=ar[i];
}
if(ar[i]>max)
{
max=ar[i];
}
System.out.println("minimum:"+min);
System.out.println("maximum:"+max);
}
}
