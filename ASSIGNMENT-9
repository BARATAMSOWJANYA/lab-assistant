#include <stdio.h>
#include<string.h>
int main(void) {
 char str[100];
 int vcount=0,ccount=0;
 printf("Enter string :");
 scanf("%s",str);
 for(int i=0;str[i]!='\0';i++)
 {
     if(str[i]=='a' || str[i]=='e' || str[i]=='i'||  str[i]=='o'||  str[i]=='u' || str[i]=='A' || str[i]=='E' ||  str[i]=='I' || str[i]=='O' || str[i]=='U')
        vcount++;
        else
        ccount++;
}
printf("vowels : %d\n",vcount);
printf("consonants :%d",ccount);
 return 0;
}
