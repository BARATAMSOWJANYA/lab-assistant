import java.util.Scanner;
class revs
{
public static void main(String args[])
{
String str,strrev="";
int len,i;
char ch;
Scanner sc=new Scanner(System.in);
System.out.println("enter the string");
str=sc.nextLine();
len=str.length();
for(i=(len-1);i>=0;i--)
{
ch=str.charAt(i);
strrev=strrev+ch;
}
System.out.println("\n reverse="+strrev);
}
}
