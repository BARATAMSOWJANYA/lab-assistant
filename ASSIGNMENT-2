int main()
{
   int n,i,count=0, a[50];
   printf("Enter number of elements:\n");
   scanf("%d",&n); 
   printf("Enter %d integers:\n", n);
   for (i = 0; i < n; i++)
   {
      scanf("%d",&a[i]); 
   }
    for(i = 0; i < n; i++) {
       
       if(a[i] > 0){
         a[count++] = a[i];
       }
        }
    while(count < n)
    {
       a[count++] = 0;
   }
   
  for(i = 0; i < n; i++) 
  {
       printf("  %d  ",a[i]);
   }
   return 0;
}
